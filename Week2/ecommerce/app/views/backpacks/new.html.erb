<%= form_for @backpack do |f| %>
	<p>
		<%= f.label :model %><br>
		<%= f.text_field :model %>
	</p>
	<p>
		<%= f.label :color %><br>
		<%= f.text_field :color %>
	</p>
	<p>
		<%= f.label :capacity %><br>
		<%= f.text_field :capacity %>
	</p>
	<p>
		<%= f.label :number_pockets %><br>
		<%= f.text_field :number_pockets %>
	</p>
	<p>
		<%= f.label :waterproof %><br>
		<%= f.text_field :waterproof %>
	</p>
	<p>
		<%= f.label :price %><br>
		<%= f.text_field :price %>
	</p>
	<p>
		<%= f.submit %>
	</p>
<% end %>
<%= link_to "Backpacks page", backpacks_path %>